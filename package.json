    "rules": {
      "no-debugger": "off",
      "no-unused-vars": "off",
      "vue/no-unused-vars": "off",
      "no-unused-components": "off",
      "vue/no-unused-components": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
